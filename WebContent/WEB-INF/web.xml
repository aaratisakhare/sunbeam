<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>restaurant1</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.sunbeam.rest.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.sunbeam.rest.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ShowCart</servlet-name>
		<servlet-class>com.sunbeam.rest.servlets.ShowCartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShowCart</servlet-name>
		<url-pattern>/showcart</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ItemsServlet</servlet-name>
		<servlet-class>com.sunbeam.rest.servlets.ItemsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ItemsServlet</servlet-name>
		<url-pattern>/items</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CatagoriesServlet</servlet-name>
		<servlet-class>com.sunbeam.rest.servlets.CatagoriesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CatagoriesServlet</servlet-name>
		<url-pattern>/categories</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddCartServlet</servlet-name>
		<servlet-class>com.sunbeam.rest.servlets.AddCartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddCartServlet</servlet-name>
		<url-pattern>/addcart</url-pattern>
	</servlet-mapping>
	<!--  <filter>
		<filter-name>SecurityFilter</filter-name>
		<filter-class>com.sunbeam.rest.filters.SecurityFilter.java</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>SecurityFilter</filter-name>
		<url-pattern>/categories</url-pattern>
	</filter-mapping>-->
</web-app>